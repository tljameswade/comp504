package sq6_kt21.concrete.chat;

import common.IChatServer;
import common.msg.chat.IAddMeMsg;

/**
 * The add me msg class
 * @author Kun Tian
 *
 */
public class AddMeMsg implements IAddMeMsg{

	/**
	 * 
	 */
	private static final long serialVersionUID = 3428537189011423279L;
	
	/**
	 * The stub of the user to be added
	 */
	IChatServer chatStub;
	
	/**
	 * The constructor for an addme msg
	 * @param _chatStub the stub of the user to be added
	 */
	public AddMeMsg(IChatServer _chatStub) {
		this.chatStub = _chatStub;
	}
	
	public IChatServer getChatStub() {
		return chatStub;
	}

}
